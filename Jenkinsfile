pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Build stage'
            }
        }
        stage('Test') { 
            steps {
                echo 'mvn Test' 
            }
        }
        stage('Clean') { 
            steps {
                echo 'Clean stage' 
            }
        }           
        
    }
}
